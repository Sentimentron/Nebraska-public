<Workflow>

	<WorkflowOptions>
		<WorkflowName>Gimpel POS Tagger</WorkflowName>
		<WorkflowDescription> Tags the tweets based on the POS tagger by Gimpel et al.</WorkflowDescription>
		<RetainOutputFile path="gimpel.sqlite"/>
		<DisableUntrackedFileCheck />
	</WorkflowOptions>

    <Tables>
        <PartOfSpeechTable name="test"/>
    </Tables>

	<InputSources>
		<PreviousWorkflow path="Workflows/SpamFight.xml" />
	</InputSources>

    <WorkflowTasks>
	<GimpelPOSTagger dest="test" verbose="true" />
    </WorkflowTasks>

</Workflow>
